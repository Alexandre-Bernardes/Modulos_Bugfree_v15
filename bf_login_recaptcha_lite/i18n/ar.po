#. module: bf_login_recaptcha_lite
msgid "Login reCAPTCHA"
msgstr "تسجيل الدخول باستخدام reCAPTCHA"

#. module: bf_login_recaptcha_lite
msgid "Chave pública (site key)"
msgstr "المفتاح العام (site key)"

#. module: bf_login_recaptcha_lite
msgid "Chave secreta (server side)"
msgstr "المفتاح السري (جهة الخادم)"

#. module: bf_login_recaptcha_lite
msgid "Falhas antes de exigir reCAPTCHA"
msgstr "عدد المحاولات الفاشلة قبل طلب reCAPTCHA"

#. module: bf_login_recaptcha_lite
msgid "Falhas antes de exigir reCAPTCHA (0 = sempre mostrar)"
msgstr "عدد المحاولات الفاشلة قبل طلب reCAPTCHA (0 = إظهار دائمًا)"

#. module: bf_login_recaptcha_lite
msgid "reCAPTCHA Site Key"
msgstr "مفتاح موقع reCAPTCHA"

#. module: bf_login_recaptcha_lite
msgid "reCAPTCHA Secret Key"
msgstr "المفتاح السري لـ reCAPTCHA"

#. module: bf_login_recaptcha_lite
msgid "Please complete the reCAPTCHA verification."
msgstr "يرجى إكمال تحقق reCAPTCHA."

#. module: bf_login_recaptcha_lite
msgid "reCAPTCHA verification failed. Please try again."
msgstr "فشل تحقق reCAPTCHA. يرجى المحاولة مرة أخرى."

#. module: bf_login_recaptcha_lite
msgid "reCAPTCHA verification error. Please try again."
msgstr "حدث خطأ في تحقق reCAPTCHA. يرجى المحاولة مرة أخرى."

